{
  "name": "@minionlab/mcp-server-demo",
  "version": "0.1.6",
  "description": "MCP server for browser automation using playwright",
  "license": "MIT",
  "author": "MinionLab",
  "homepage": "https://minionlab.com",
  "bugs": "https://github.com/minionlab/mcp-server-demo/issues",
  "type": "module",
  "bin": {
    "mcp-server-demo": "dist/src/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && node -e \"import('node:fs').then(fs => fs.chmodSync('dist/src/index.js', '755'))\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "start": "node dist/src/index.js",
    "inspector": "node node_modules/@modelcontextprotocol/inspector/bin/cli.js dist/src/index.js",
    "npx-inspector": "npx @modelcontextprotocol/inspector dist/src/index.js",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@modelcontextprotocol/inspector": "^0.4.1",
    "@modelcontextprotocol/sdk": "^1.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "playwright": "1.50"
  },
  "devDependencies": {
    "@types/chai": "^5.0.1",
    "@types/chai-as-promised": "^8.0.1",
    "@types/eventsource": "^3.0.0",
    "chai": "^5.2.0",
    "chai-as-promised": "^8.0.1",
    "eventsource": "^3.0.5",
    "node-fetch": "^2.7.0",
    "patch-package": "^8.0.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
